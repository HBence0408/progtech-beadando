<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx/17.0.12"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.gatchasim.gatchasim.JavaFX.MainController"
      spacing="10.0"
      prefHeight="402.0" prefWidth="632.0">

   <HBox spacing="10.0">
      <padding>
         <Insets top="10.0" right="10.0" bottom="0.0" left="10.0"/>
      </padding>

      <Button fx:id="CCButton"
              text="Cookie Clicker"
              onAction="#ShowCC"
              HBox.hgrow="ALWAYS"
              maxWidth="Infinity"
              prefHeight="26.0" />
      <Button fx:id="InventoryButton"
              text="Inventory"
              onAction="#ShowInventory"
              HBox.hgrow="ALWAYS"
              maxWidth="Infinity"
              prefHeight="26.0" />

      <Button fx:id="Logout_Button"
              text="Logout"
              onAction="#Logout"
              prefWidth="100.0"
              prefHeight="26.0"
              style="-fx-background-color: red;"
              textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </HBox>


   <Separator VBox.vgrow="ALWAYS"/>
</VBox>


